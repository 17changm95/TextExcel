package textExcel;

public class ValueCell extends RealCell implements Cell {
	
	public ValueCell(String str) {
		super(str);
	}
	
	public String abbreviatedCellText() {
		return super.abbreviatedCellText();
	}
	
	public String fullCellText() {
		return super.fullCellText();
	}
	
	@Override
	public double getDoubleValue() {
		String modifiedStr = super.fullCellText().substring(0, super.fullCellText().length() - 2);
		double num = Double.parseDouble(modifiedStr);
		return num;
	}
}
